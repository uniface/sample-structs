<DAT name="DETAIL" xml:space='preserve'>variables
struct compStruct, orderStruct, orderPositionStruct, ordersPerCustomer, orderPositionsPerOrder
numeric c, o, op
endvariables

orderStruct = $newstruct
orderStruct -&gt; $name = "ORDER"
orderPositionStruct = $newstruct
orderPositionStruct -&gt; $name = "ORDERPOSITION"

compStruct = $struct1$ -&gt; DAILYORDERS
compStruct -&gt; $name = "CUSTOMER"
compStruct -&gt; CUSTOMER -&gt; $name = "OCC"
compstruct -&gt; OCC -&gt; ORDER -&gt; $name = "OCC"

for c = 1 to compstruct -&gt; OCC -&gt; $collsize
	compstruct -&gt; OCC{c} -&gt;*{-1} = orderstruct
	compstruct -&gt; OCC{c} -&gt; ORDER -&gt; $index = 8
	ordersPerCustomer = $newstruct
	compstruct -&gt; OCC{c} -&gt; OCC -&gt; $parent = ordersPerCustomer
	compstruct -&gt; OCC{c} -&gt; ORDER = ordersPerCustomer

	compstruct -&gt; OCC{c} -&gt; ORDER -&gt; OCC -&gt; ORDERPOSITION -&gt; $name = "OCC"
 	for o = 1 to compstruct -&gt; OCC{1} -&gt; ORDER -&gt; OCC -&gt; $collsize
		compstruct -&gt; OCC{c} -&gt; ORDER -&gt; OCC{o} -&gt;*{-1} = orderPositionStruct
		compstruct -&gt; OCC{c} -&gt; ORDER -&gt; OCC{o} -&gt; ORDERPOSITION -&gt; $index = 4
		orderPositionsPerOrder = $newstruct
		compstruct -&gt; OCC{c} -&gt; ORDER -&gt; OCC{o} -&gt; OCC -&gt; $parent = orderPositionsPerOrder
		compstruct -&gt; OCC{c} -&gt; ORDER -&gt; OCC{o} -&gt; ORDERPOSITION = orderPositionsPerOrder
	endfor
endfor

structvar.dummy = compstruct -&gt; $dbgstring
structToComponent compstruct
$struct1$ = compStruct

if ($status &lt; 0)
	message/info $procerrorcontext
endif

setocc "customer",1
setocc "order",1
setocc "orderposition",1

