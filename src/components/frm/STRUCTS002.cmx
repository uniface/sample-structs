<DAT name="INITVALUE" xml:space='preserve'>vStructCD = $struct1$ -&gt; CATALOG -&gt; CD
vCollSize = vStructCD -&gt; $collSize
for vCurNode = 1 to vCollSize
	vStructCD{vCurNode} -&gt; TITLE -&gt; $tags -&gt; xmlClass = "attribute"
	vStructCD{vCurNode} -&gt; PRICES = $newstruct
	vStructCD{vCurNode} -&gt; PRICE -&gt; $parent = vStructCD{vCurNode} -&gt; PRICES
	vStructPrices = vStructCD{vCurNode} -&gt; PRICES
	vStructPrices -&gt; PRICE -&gt; CURRENCY = $newstruct
	vStructPrices -&gt; PRICE -&gt; CURRENCY -&gt; $index = 1
	vStructPrices -&gt; PRICE -&gt; CURRENCY = "USD"
	vStructPrices -&gt; PRICE -&gt; CURRENCY -&gt; $tags -&gt; xmlClass = "attribute"
	vStructPrice = vStructPrices -&gt; PRICE
	vStructPrices -&gt; PRICE{-1} = vStructPrice
	vStructPrice2 = vStructPrices -&gt; PRICE{-1}
	vStructPrice2 -&gt; CURRENCY = "EUR"
	vStructPrice2 -&gt; $scalar = ($number(vStructPrice-&gt;$scalar) * 0.76 * 1.2)[round,1]
	vStructPrices -&gt; PRICE{-1} = vStructPrice
	vStructPrice2 = vStructPrices -&gt; PRICE{-1}
	vStructPrice2 -&gt; CURRENCY = "GBP"
	vStructPrice2 -&gt; $scalar = ($number(vStructPrice-&gt;$scalar) * 0.64 * 1.2)[round,1]
endfor
