<OCC>
<DAT name="UTIMESTAMP">2013-06-29T18:05:11.13</DAT>
<DAT name="ULABEL">BTN3</DAT>
<DAT name="GRP">DUMMY</DAT>
<DAT name="UBASE">DUMMY</DAT>
<DAT name="UFORM">STRUCTS002</DAT>
<DAT name="U_DTYP">S</DAT>
<DAT name="U_INDB">X</DAT>
<DAT name="INTERFACEMOD">C100</DAT>
<DAT name="WIDGETTYPE">COMMANDBUTTON</DAT>
<DAT name="UINHERIT">F</DAT>
<DAT name="DETAIL" xml:space='preserve'>variables
	struct vStructCD, vStructPrices, vStructPrice, vStructPrice2
	numeric xcngUSDtoEUR, xCngUSDtoGBP, vCollSize, vCurNode, vPriceEUR
endvariables

xCngUSDtoEUR = 0.762427569
xCngUSDtoGBP = 0.647374895

vStructCD = $struct1$ -&gt; CATALOG -&gt; CD
vCollSize = vStructCD -&gt; $collSize

message/info "Step 1: Change element [TITLE] into attribute"

vStructCD{1} -&gt; TITLE -&gt; $tags -&gt; xmlClass = "attribute"

call showStruct()
message/info "Step 2: Create struct member [PRICES]"

vStructCD{1} -&gt; PRICES = $newstruct

call showStruct()
message/info "Step 3: Move [CD]-&gt;[PRICE] to [CD]-&gt;[PRICES]"

vStructCD{1} -&gt; PRICE -&gt; $parent = vStructCD{1} -&gt; PRICES

call showStruct()
message/info "Step 4: Create an attribute called [CURRENCY] for [PRICE] with value %%"USD%%""

vStructPrices = vStructCD{1} -&gt; PRICES
vStructPrices -&gt; PRICE -&gt; CURRENCY = $newstruct
vStructPrices -&gt; PRICE -&gt; CURRENCY -&gt; $index = 1
vStructPrices -&gt; PRICE -&gt; CURRENCY = "USD"
vStructPrices -&gt; PRICE -&gt; CURRENCY -&gt; $tags -&gt; xmlClass = "attribute"

call showStruct()
message/info "Step 5: Create a second [PRICE] member with value %%"EUR%%" by copying the first one"

vStructPrice = vStructPrices -&gt; PRICE
vStructPrices -&gt; PRICE{-1} = vStructPrice
vStructPrice2 = vStructPrices -&gt; PRICE{-1}
vStructPrice2 -&gt; CURRENCY = "EUR"
vStructPrice2 -&gt; $scalar = ($number(vStructPrice-&gt;$scalar) * xCngUSDtoEUR * 1.2)[round,1]

call showStruct()
message/info "Step 6: Create a third [PRICE] member with value %%"GBP%%" by copying the first one"

vStructPrices -&gt; PRICE{-1} = vStructPrice
vStructPrice2 = vStructPrices -&gt; PRICE{-1}
vStructPrice2 -&gt; CURRENCY = "GBP"
vStructPrice2 -&gt; $scalar = ($number(vStructPrice-&gt;$scalar) * xCngUSDtoGBP * 1.2)[round,1]

call showStruct()
message/info "Step 7: Now we do the above for all [CD] members"

for vCurNode = 2 to vCollSize
	vStructCD{vCurNode} -&gt; TITLE -&gt; $tags -&gt; xmlClass = "attribute"
	vStructCD{vCurNode} -&gt; PRICES = $newstruct
	vStructCD{vCurNode} -&gt; PRICE -&gt; $parent = vStructCD{vCurNode} -&gt; PRICES
	vStructPrices = vStructCD{vCurNode} -&gt; PRICES
	vStructPrices -&gt; PRICE -&gt; CURRENCY = $newstruct
	vStructPrices -&gt; PRICE -&gt; CURRENCY -&gt; $index = 1
	vStructPrices -&gt; PRICE -&gt; CURRENCY = "USD"
	vStructPrices -&gt; PRICE -&gt; CURRENCY -&gt; $tags -&gt; xmlClass = "attribute"
	vStructPrice = vStructPrices -&gt; PRICE
	vStructPrices -&gt; PRICE{-1} = vStructPrice
	vStructPrice2 = vStructPrices -&gt; PRICE{-1}
	vStructPrice2 -&gt; CURRENCY = "EUR"
	vStructPrice2 -&gt; $scalar = ($number(vStructPrice-&gt;$scalar) * xCngUSDtoEUR * 1.2)[round,1]
	vStructPrices -&gt; PRICE{-1} = vStructPrice
	vStructPrice2 = vStructPrices -&gt; PRICE{-1}
	vStructPrice2 -&gt; CURRENCY = "GBP"
	vStructPrice2 -&gt; $scalar = ($number(vStructPrice-&gt;$scalar) * xCngUSDtoGBP * 1.2)[round,1]
endfor

call showStruct()
</DAT>
