    <init-param>
       <param-name>TESTABLE</param-name>
       <param-value>true</param-value>
    </init-param>
    <init-param>
       <param-name>MIDDLEWARE</param-name>
      <param-value>UV8:localhost+13001|userver|Cowabunga1|UnifaceStructs_debug</param-value>
    </init-param>
  </servlet>

