.L=ulabel(transparency=off;labelfont=label;inheritcolors=false;anchor=false)
.P=ucmdbutton(tooltip=on;autolabel=off)
.C=ucheckbox(v52=on)
.I=upicture

[gfp]
; Logical widgets to appear on the main floating palette
