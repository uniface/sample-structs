; Custom widgets for development environment
RichEdit=urichedit(font=procboxfont;maxtags=20;procbox=on)
HtmlEdit=urichedit(font=procboxfont;procbox=on;language=html)
HotSpot=uhotspot(cursor=on)
UniForm=uniform
Nav=unav(font=label)
Gab=ugab
UComposer=ucomposer

